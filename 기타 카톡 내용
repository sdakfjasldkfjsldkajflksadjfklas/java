import java.util.Scanner;
class Person{
   private String name;
   private int id;
   Person(String a ,int b){
      this.name=a;
      this.id=b;
   }
    String getName() {
      return name;
   }
    int getId() {
      return id;
   }   
   public boolean equals(Object obj) {
      Person p=(Person)obj;
      
      if(p.getId()==this.id && p.getName().equals(this.name)) {
         return true;
      }
      else {
         return false;
      }
   }
   
}


   static int[] con(int ary1[], int ary2[]) {
      
      int []arr=new int[ary1.length + ary2.length];
      //int []arr=new int[8];
      for(int i=0;i<ary1.length;i++) {
         arr[i]=ary1[i]; 
         for(int j=ary1.length;j<ary1.length + ary2.length;j++) {
            //j=4~7
            arr[j]=ary2[j - ary1.length];
         }
      }
      return arr;
   }

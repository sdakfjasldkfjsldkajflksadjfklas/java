      id=i;
   }
   String getname() {
      return name;
   }
   int getid() {
      return id;
   }
   public boolean equals(Object obj) {
      Person p=(Person)obj;
      
      if(p.getid()==this.id && p.getname().equals(this.name)) {
         return true;
      }
      return false;
   }
}
